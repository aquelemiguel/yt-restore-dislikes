{
  "name": "YT Restore Dislikes",
  "description": "Restore the dislike count on your YouTube videos.",
  "version": "1.1",
  "manifest_version": 2,
  "background": {
    "scripts": ["service.js"]
  },
  "permissions": [
    "tabs",
    "webNavigation",
    "*://*.youtube.com/*",
    "*://*.googleapis.com/youtube/v3/*"
  ],
  "icons": {
    "16": "/icons/icon_16.png",
    "32": "/icons/icon_32.png",
    "48": "/icons/icon_48.png",
    "128": "/icons/icon_128.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "aquelemiguel@yt-see-dislikes"
    }
  }
}
